    "\n",
    "fname = input('Enter File name :')\n",
    "\n",
    "handle = open(fname)\n",
    "\n",
    "sum=0\n",
    "\n",
    "count = 0\n",
    "\n",
    "for line in handle:\n",
    "\t\n",
    "\tf = re.findall('[0-9]+',line)\n",
    "\t\n",
    "\tfor num in f:\n",
    "\t\t\n",
    "\t\tif int(num) >= 0:\n",
    "\t\t\t\n",
    "\t\t\tcount = count + 1\n",
    "\t\t\tsum = sum + int(num)\n",
    "\t\t\n",
    "print('There are',count,'values with a sum =',sum)"
